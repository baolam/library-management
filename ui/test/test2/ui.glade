<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <object class="GtkWindow" id="main_window">
    <property name="default_width">600</property>
    <property name="default_height">400</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkStackSwitcher" id="stack_switcher">
            <property name="stack">main_stack</property>
          </object>
        </child>
        <child>
          <object class="GtkStack" id="main_stack">
            <property name="transition-type">slide-left-right</property>
            <child>
              <object class="GtkBox" id="login_page">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label">Đăng nhập</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="name">login</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox" id="main_page">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label">Giao diện chính</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="name">main</property>
              </packing>
            </child>
            <child>
              <object class="GtkBox" id="manual_page">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkLabel">
                    <property name="label">Hướng dẫn sử dụng</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="name">manual</property>
              </packing>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
